<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Label for item that reboots the device in device options dialog -->
    <string name="global_action_reboot">重新启动</string>
    <!-- Button to reboot the device, within the reboot options dialog -->
    <string name="reboot_reboot">@string/global_action_reboot</string>
    <!-- Button to reboot the device into recovery, within the reboot options dialog -->
    <string name="reboot_recovery">恢复模式</string>
    <!-- Button to reboot the device into fastboot, within the reboot options dialog -->
    <string name="reboot_fastboot">刷机模式</string>

    <!-- Reboot confirmation dialog title -->
    <string name="reboot_system" product="tablet">重启平板</string>
    <string name="reboot_system" product="default">重启手机</string>
    <!-- Reboot confirmation dialog.  This is the message shown to the user when he chooses to reboot the device -->
    <string name="reboot_confirm" product="tablet">你的平板将会重启</string>
    <string name="reboot_confirm" product="default">你的手机将会重启</string>

    <!-- Reboot progress dialog. This is shown if the user chooses to reboot the device. -->
    <string name="reboot_progress">正在重启\u2026</string>

    <!-- Label for item that screenshots in device options dialog -->
    <string name="global_action_screenshot">屏幕截图</string>
</resources>
