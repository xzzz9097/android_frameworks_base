<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Label for item that reboots the device in device options dialog -->
    <string name="global_action_reboot">Reiniciar</string>
    <!-- Button to reboot the device, within the reboot options dialog -->
    <string name="reboot_reboot">@string/global_action_reboot</string>
    <!-- Button to reboot the device into recovery, within the reboot options dialog -->
    <string name="reboot_recovery">Recuperação</string>
    <!-- Button to reboot the device into fastboot, within the reboot options dialog -->
    <string name="reboot_fastboot">Fastboot</string>

    <!-- Reboot confirmation dialog title -->
    <string name="reboot_system" product="tablet">Reiniciar tablet</string>
    <string name="reboot_system" product="default">Reiniciar telefone</string>
    <!-- Reboot confirmation dialog.  This is the message shown to the user when he chooses to reboot the device -->
    <string name="reboot_confirm" product="tablet">Seu tablet irá reiniciar.</string>
    <string name="reboot_confirm" product="default">Seu telefone irá reiniciar.</string>

    <!-- Reboot progress dialog. This is shown if the user chooses to reboot the device. -->
    <string name="reboot_progress">Reiniciando\u2026</string>

    <!-- Label for item that screenshots in device options dialog -->
    <string name="global_action_screenshot">Captura de tela</string>

    <!-- Setting confirmation dialog strings. Shown when any UI interaction can be modified by user input -->
    <string name="setting_confirmation_ask_me_later">Perguntar depois</string>
    <string name="setting_confirmation_yes">Sim</string>
    <string name="setting_confirmation_no">Não</string>

    <!-- Immersive mode confirmation: Pick confirmation window from bottom when status bar is visible -->
    <string name="immersive_mode_confirmation_bottom">Deslizar para cima a partir do fundo para sair da tela cheia.</string>

    <!-- Safe headset volume warning dialog -->
    <string name="safe_headset_warning_title">Volume Seguro do fone de ouvido</string>
    <string name="safe_headset_warning_message">Gostaria de desativar o aviso do nível de volume recomendado?</string>
</resources>
