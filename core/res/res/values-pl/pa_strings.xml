<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Label for item that reboots the device in device options dialog -->
    <string name="global_action_reboot">Uruchom ponownie</string>
    <!-- Button to reboot the device into recovery, within the reboot options dialog -->
    <string name="reboot_recovery">Recovery</string>
    <!-- Button to reboot the device into fastboot, within the reboot options dialog -->
    <string name="reboot_fastboot">Fastboot</string>

    <!-- Reboot confirmation dialog title -->
    <string name="reboot_system" product="tablet">Uruchom tablet ponownie</string>
    <string name="reboot_system" product="default">Uruchom telefon ponownie</string>
    <!-- Reboot confirmation dialog.  This is the message shown to the user when he chooses to reboot the device -->
    <string name="reboot_confirm" product="tablet">Twój tablet uruchomi się ponownie.</string>
    <string name="reboot_confirm" product="default">Twój telefon uruchomi się ponownie..</string>

    <!-- Reboot progress dialog. This is shown if the user chooses to reboot the device. -->
    <string name="reboot_progress">Ponowne uruchamianie\u2026</string>

    <!-- Label for item that screenshots in device options dialog -->
    <string name="global_action_screenshot">Zrzut ekranu</string>

    <!-- Setting confirmation dialog strings. Shown when any UI interaction can be modified by user input -->
    <string name="setting_confirmation_ask_me_later">Zapytaj mnie później</string>
    <string name="setting_confirmation_yes">Tak</string>
    <string name="setting_confirmation_no">Nie</string>

    <!-- Immersive mode confirmation: Pick confirmation window from bottom when status bar is visible -->
    <string name="immersive_mode_confirmation_bottom">Przeciągnij z dołu do góry aby opuścić tryb pełnoekranowy.</string>

    <!-- Safe headset volume warning dialog -->
    <string name="safe_headset_warning_title">Bezpieczna głośność słuchawek</string>
    <string name="safe_headset_warning_message">Czy chciałbyś wyłączyć ostrzeżenie o poziomie głośności?</string>
</resources>
