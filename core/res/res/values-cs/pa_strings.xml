<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Label for item that reboots the device in device options dialog -->
    <string name="global_action_reboot">Restartovat</string>
    <!-- Button to reboot the device, within the reboot options dialog -->
    <string name="reboot_reboot">@string/global_action_reboot</string>
    <!-- Button to reboot the device into recovery, within the reboot options dialog -->
    <string name="reboot_recovery">Recovery</string>
    <!-- Button to reboot the device into fastboot, within the reboot options dialog -->
    <string name="reboot_fastboot">Fastboot</string>

    <!-- Reboot confirmation dialog title -->
    <string name="reboot_system" product="tablet">Restartovat tablet</string>
    <string name="reboot_system" product="default">Restartovat telefon</string>
    <!-- Reboot confirmation dialog.  This is the message shown to the user when he chooses to reboot the device -->
    <string name="reboot_confirm" product="tablet">Váš tablet se restartuje.</string>
    <string name="reboot_confirm" product="default">Váš telefon se restartuje.</string>

    <!-- Reboot progress dialog. This is shown if the user chooses to reboot the device. -->
    <string name="reboot_progress">Restartuji\u2026</string>

    <!-- Label for item that screenshots in device options dialog -->
    <string name="global_action_screenshot">Snímek obrazovky</string>

    <!-- Setting confirmation dialog strings. Shown when any UI interaction can be modified by user input -->
    <string name="setting_confirmation_ask_me_later">Později</string>
    <string name="setting_confirmation_yes">Ano</string>
    <string name="setting_confirmation_no">Ne</string>

    <!-- Immersive mode confirmation: Pick confirmation window from bottom when status bar is visible -->
    <string name="immersive_mode_confirmation_bottom">Tažením odspodu obrazovky směrem nahoru ukončíte režim celé obrazovky.</string>

    <!-- Safe headset volume warning dialog -->
    <string name="safe_headset_warning_title">Bezpečná hlasitost sluchátek</string>
    <string name="safe_headset_warning_message">Chtěli byste vypnout varování o bezpečné úrovni hlasitosti?</string>
</resources>
