<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Label for item that reboots the device in device options dialog -->
    <string name="global_action_reboot">Перезагрузка</string>
    <!-- Button to reboot the device into recovery, within the reboot options dialog -->
    <string name="reboot_recovery">Режим восстановления</string>
    <!-- Button to reboot the device into fastboot, within the reboot options dialog -->
    <string name="reboot_fastboot">Загрузчик</string>

    <!-- Reboot confirmation dialog title -->
    <string name="reboot_system" product="tablet">Перезагрузить планшет</string>
    <string name="reboot_system" product="default">Перезагрузить телефон</string>
    <!-- Reboot confirmation dialog.  This is the message shown to the user when he chooses to reboot the device -->
    <string name="reboot_confirm" product="tablet">Ваш планшет будет перезагружен.</string>
    <string name="reboot_confirm" product="default">Ваш телефон будет перезагружен.</string>

    <!-- Reboot progress dialog. This is shown if the user chooses to reboot the device. -->
    <string name="reboot_progress">Перезагрузка\u2026</string>

    <!-- Label for item that screenshots in device options dialog -->
    <string name="global_action_screenshot">Снимок экрана</string>

    <!-- Setting confirmation dialog strings. Shown when any UI interaction can be modified by user input -->
    <string name="setting_confirmation_ask_me_later">Спросить позже</string>
    <string name="setting_confirmation_yes">Да</string>
    <string name="setting_confirmation_no">Нет</string>

    <!-- Immersive mode confirmation: Pick confirmation window from bottom when status bar is visible -->
    <string name="immersive_mode_confirmation_bottom">Проведите снизу вверх, чтобы вернуться в полноэкранный режим.</string>

    <!-- Safe headset volume warning dialog -->
    <string name="safe_headset_warning_title">Безопасная громкость</string>
    <string name="safe_headset_warning_message">Вы хотите отключить предупреждение о безопасном уровне громкости?</string>
</resources>
